# EditorConfig
# http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
end_of_line = lf
trim_trailing_whitespace = true

# JavaScript npm/yarn package.json
[{package.json}]
indent_style = space
indent_size = 2