# EditorConfig
# http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
charset = utf-8

# tab indentation
indent_style = tab

# JavaScript npm/yarn package.json
[{package.json}]
indent_style = space
indent_size = 2